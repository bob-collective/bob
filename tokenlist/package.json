{
  "name": "@gobob/tokenlist",
  "version": "1.0.0",
  "description": "BOB token list data and utilities",
  "license": "MIT",
  "main": "index.ts",
  "files": [
    "index.ts",
    "config.ts",
    "types.ts",
    "utils.ts",
    "generated-types.ts",
    "tokenlist.json",
    "tokenlist*.json"
  ],
  "scripts": {
    "build": "npm run generate:types && npm run build:tokenlist",
    "build:tokenlist": "tsx ./scripts/generate.ts",
